{
  "multipart": [
    {
      "when": {
        "OR": [
          {
            "north": false,
            "south": false,
            "east": false,
            "west": false
          }
        ]
      },
      "apply": {
        "model": "arstheurgia:block/flour_dust_dot"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "arstheurgia:block/flour_dust_side0"
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "arstheurgia:block/flour_dust_side_alt0"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "arstheurgia:block/flour_dust_side_alt1",
        "y": 270
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "arstheurgia:block/flour_dust_side1",
        "y": 270
      }
    }

  ]
}